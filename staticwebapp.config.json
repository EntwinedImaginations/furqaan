{
  "routes": [
    { "route": "/privacy", "rewrite": "/privacy/index.html" }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/privacy/*"]
  },
  "responseOverrides": {
    "404": { "rewrite": "/index.html", "statusCode": 404 }
  },
  "globalHeaders": {
    "x-frame-options": "DENY",
    "x-content-type-options": "nosniff",
    "referrer-policy": "strict-origin-when-cross-origin"
  }
}
