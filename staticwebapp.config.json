{
  "routes": [
    {
      "route": "/",
      "redirect": "https://www.nurtaj.com.au",
      "statusCode": 301
    },
    {
      "route": "/privacy",
      "rewrite": "/privacy/index.html"
    }
  ],
  "globalHeaders": {
    "Strict-Transport-Security": "max-age=31536000; includeSubDomains; preload"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/privacy/*"]
  }
}
